package archersysos.jawa.io.filters;

import java.io.File;

import javax.swing.filechooser.FileFilter;

public class JavaScriptFilter extends FileFilter {

	@Override
	public boolean accept(File arg0) {
		return arg0.getName().contains(".js");
	}

	@Override
	public String getDescription() {
		return "Javascript Program";
	}

}
