<?xml version="1.0" encoding="utf-8"?>
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.1">
	<CommandSet xml:lang="en-us" Name="ArcherVM.Windows_en-us">
		<CommandPrefix>DrCortana</CommandPrefix>
		<Example>Start PDFLint</Example>

    <Command Name="PDFLintStarter">
      <Example>Start PDFLint</Example>
      <ListenFor>Start {App}</ListenFor>
      <Feedback> {App} is now opening...</Feedback>
      <Navigate Target="PDFLint.xaml"/>
    </Command>
    <PhraseList Label="App">
      <Item>PDFLint</Item>
    </PhraseList>
	</CommandSet>
    
	<!-- Other CommandSets for other languages -->

</VoiceCommands>